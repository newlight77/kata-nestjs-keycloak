{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:remote": "nest start --debug 0.0.0.0:3080 --watch",
    "start:prod": "node dist/main",
    "start:dev:db": "./run.db.sh",
    "start:dev:db:seed": "ts-node -r tsconfig-paths/register ./src/scripts/data.seeds.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "nyc:coverage:unit": "nyc --silent jest",
    "nyc:coverage:domain": "nyc --reporter=lcov --reporter=text npm run test:features:domain",
    "nyc:coverage:interface": "nyc --reporter=lcov --reporter=text npm run test:features:interface",
    "nyc:coverage:merge": "npm run nyc:coverage:domain && npm run nyc:coverage:interface && nyc merge .nyc_output coverage/coverage_cucumber.json",
    "codecov": "codecov -t ff93e55e-70e5-4fef-96c2-a4a532282ee1",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:features:domain": "cucumber-js -p profileDomain",
    "test:features:interface": "cucumber-js -p profileInterface",
    "typeorm:config": "ts-node -r tsconfig-paths/register ./src/scripts/typeorm-config-to-file.ts",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm:cli -- migration:generate -n",
    "typeorm:migration:run": "npm run typeorm:cli -- migration:run"
  },
  "dependencies": {
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@nestjs/axios": "^0.0.6",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "axios": "^0.26.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "keycloak-connect": "^17.0.0",
    "nest-keycloak-connect": "^1.8.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.43"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/better-sqlite3": "^7.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "better-sqlite3": "^7.5.1",
    "chai": "^4.3.6",
    "cucumber-jest": "^0.3.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text-summary",
      "json-summary",
      "lcov"
    ],
    "testEnvironment": "node"
  },
  "nyc": {
    "all": true,
    "require": "ts-node/register",
    "instrument": true,
    "report-dir": "./coverage",
    "reporter": ["lcov", "text"],
    "source-map": true,
    "produce-source-map": false,
    "check-coverage": false,
    "exclude": [
      "test/**",
      "**/*.spec.ts",
      "coverage/**",
      "node_modules/**",
      "*.js"
    ],
    "excludeNodeModules": true
  }
}
